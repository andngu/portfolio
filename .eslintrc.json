{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "react/react-in-jsx-scope": "off",
        "react/jsx-uses-react": "off",
        "prettier/prettier": "error",
        "react/jsx-filename-extension": [0]
    }
}